{
    "$schema": "vendor/infection/infection/resources/schema.json",
    "source": {
        "directories": [
            "app",
            "src"
        ]
    },
    "logs": {
        "text": "infection.log"
    },
    "mutators": {
        "@default": true,
        "ProtectedVisibility": {
            "ignore": [
                "Iteo\\Shared\\DomainEvent\\DomainEventsTrait"
            ]
        },
        "IncrementInteger": {
            "ignore": [
                "Iteo\\Customer\\Domain\\ValueObject\\Order\\Order::validateOrderWeight"
            ]
        },
        "UnwrapArrayReduce": {
            "ignore": [
                "Iteo\\Customer\\Domain\\ValueObject\\Order\\Order::validateOrderSize"
            ]
        }
    }
}
